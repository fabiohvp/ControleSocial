{
	"name": "svelte-app",
	"version": "1.0.0",
	"scripts": {
		"dev": "run-p routify rollup:watch",
		"dev:nollup": "run-p routify nollup:watch",
		"dev:dynamic": "cross-env BUNDLING=dynamic npm run dev:nollup",
		"build": "routify -b && rollup -c && routify export && workbox injectManifest",
		"serve": "spassr --serve-spa --serve-ssr",
		"rollup:watch": "rollup -cw",
		"nollup:watch": "nollup -c",
		"routify": "routify"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^2.2.0",
		"@rollup/plugin-commonjs": "^12.0.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@sveltech/routify": "^1.9.2",
		"autoprefixer": "^9.8.0",
		"cross-env": "^7.0.2",
		"del": "^5.1.0",
		"http-server": "^0.12.3",
		"node-sass": "^4.14.1",
		"nollup": "^0.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.1",
		"postcss-import": "^12.0.1",
		"postcss-url": "^8.0.0",
		"ppid-changed": "^1.0.1",
		"rollup": "^2.10.5",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-hot": "^0.0.31",
		"rollup-plugin-iife": "^0.3.1",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-postcss-hot": "^0.0.5",
		"rollup-plugin-svelte": "^5.2.2",
		"rollup-plugin-svelte-hot": "^0.9.2",
		"rollup-plugin-terser": "^5.3.0",
		"rollup-plugin-workbox": "^5.0.1",
		"run-p": "^0.0.0",
		"sirv-cli": "^0.4.5",
		"spassr": "^1.1.2",
		"svelte": "^3.49.0",
		"svelte-preprocess": "^3.4.0",
		"tailwindcss": "^1.4.6",
		"workbox": "^0.0.0"
	},
	"dependencies": {
		"@fabiohvp/svelte-pdfjs": "^0.0.14",
		"@fabiohvp/svelte-table": "^1.6.7",
		"@fortawesome/free-regular-svg-icons": "^5.13.0",
		"@fortawesome/free-solid-svg-icons": "^5.13.0",
		"axios": "^0.19.2",
		"echarts": "^4.8.0",
		"fa-svelte": "^3.1.0",
		"pdfjs-dist": "^2.3.200"
	},
	"routify": {
		"extensions": "svelte,html",
		"dynamicImports": false
	}
}